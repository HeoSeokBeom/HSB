<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="Data">
   <typeAlias alias="MemberVo" type="com.listen.member.vo.MemberVo" />

   <select id="getEmailList" resultClass="MemberVo" parameterClass="java.lang.String">
      SELECT EMAIL, PASSWORD FROM MEMBERS WHERE EMAIL = #id#  
   </select>
   
	<select id="getPassList" resultClass="MemberVo" parameterClass="java.lang.String">
      SELECT EMAIL FROM MEMBERS WHERE PASSWORD = #pass# 
   </select>
   
	
</sqlMap>